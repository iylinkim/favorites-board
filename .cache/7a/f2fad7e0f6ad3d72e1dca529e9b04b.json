90%;\n  margin: 0 auto;\n  display: flex;\n  flex-direction: column;\n}\n\n/* header */\n.banner {\n  width: 100%;\n  padding: 70px 0 20px;\n  border-bottom: 2px dashed #025955;\n  text-align: center;\n  /* background-color:rgb(2, 89, 85, 0.5); */\n}\n\n.banner_title {\n  font-size: 2.5rem;\n}\n\n.controls {\n  width: 80%;\n  margin: 30px auto 0;\n  display: flex;\n  justify-content: space-between;\n}\n\n.control {\n  width: 30%;\n}\n\n.control_btn {\n  display: block;\n  width: 100%;\n  padding: 10px 0;\n  border-radius: 5px;\n  font-size: 1.3rem;\n  color: #efefef;\n  cursor: pointer;\n  background-color: #025955;\n  transition: all 0.2s ease;\n}\n\n.control_btn:hover {\n  opacity: 0.8;\n}\n\n/* document */\n.document {\n  height: 100%;\n}\n\n.page {\n  width: 90%;\n  margin: 40px auto 0;\n}\n\n/* document / page_item */\n.page_item {\n  padding: 15px;\n  margin-bottom: 25px;\n  border-radius: 10px;\n  border:3px solid transparent;\n  background-color: rgb(248, 248, 248);\n  box-shadow: 0 0 10px rgba(0, 0, 0, 0.15);\n  position: relative;\n  transition: all 0.7s ease;\n  animation: slide 0.7s ease;\n}\n\n@keyframes slide {\n  0% {\n    transform: translateX(150px);\n  }\n  100% {\n    transform: translateX(0px);\n  }\n}\n\n.page_item_controls {\n  position: absolute;\n  right: 15px;\n  top: 15px;\n  cursor: pointer;\n}\n\n/* document / page_item_body */\n.page_item_body > .item {\n  display: flex;\n  justify-content: space-between;\n}\n\n.holder {\n  width: 43%;\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  background-color: green;\n}\n\n.text {\n  width: 55%;\n  font-size: 1.2rem;\n  color: #482c06;\n}\n\n.text .title {\n  padding-top: 10px;\n  font-size: 1.4em;\n}\n\n.text .info {\n  padding-top: 10px;\n  font-size: 0.9em;\n}\n\n/* place */\n.place_address {\n  display: flex;\n  padding-top: 5px;\n  font-size: 0.8em;\n}\n.place_address .pin {\n  padding-right: 5px;\n  color: #022422;\n}\n\n.place_holder > img {\n  width: 100%;\n}\n\n/* video */\n.video_iframe {\n  width: 100%;\n  height: 100%;\n}\n\n/* note */\n.daily,\n.important,\n.todo {\n  display: inline-block;\n  margin-top: 15px;\n  padding: 4px 12px 1px;\n  border-radius: 10px;\n  font-size: 0.8rem;\n  background-color: #addacb;\n}\n\n.important {\n  background-color: #bda8d6;\n}\n\n.todo {\n  background-color: #e4af91f3;\n}\n\n/* dialog */\n.dialog {\n  width: 100%;\n  height: 100%;\n  position: fixed;\n  z-index: 1;\n  display: flex;\n  justify-content: center;\n  align-items: center;\n  background-color: rgba(0, 0, 0, 0.45);\n  transition: opacity 0.2s ease;\n  animation: opa 0.3s ease;\n}\n\n@keyframes opa {\n  0% {\n    opacity: 0;\n  }\n  100% {\n    opacity: 1;\n  }\n}\n\n#dialog_body {\n  max-width: 810px;\n  width: 50%;\n  padding: 20px;\n  height: 300px;\n  margin: 0 auto;\n  border-radius: 10px;\n  position: relative;\n  background-color: #efefef;\n  transition: transform 0.2s ease;\n  animation: size 0.3s ease;\n}\n\n@keyframes size {\n  0% {\n    transform: scale(0.5);\n  }\n  100% {\n    transform: scale(1);\n  }\n}\n\n.category_title {\n  font-size: 1.9rem;\n  letter-spacing: 1px;\n  color: #022422;\n  padding-bottom: 20px;\n}\n\n.form_container {\n  margin-bottom: 15px;\n}\n\n#dialog_body label[for=\"title\"] {\n  padding-right: 10px;\n  font-size: 1.1rem;\n  color: #022422;\n  letter-spacing: 0.5px;\n}\n\n#title,\n#comment,\n#select,\n#url {\n  width: 100%;\n  background-color: #ddd;\n  border-radius: 3px;\n  line-height: 30px;\n  vertical-align: middle;\n}\n\n#select {\n  width: auto;\n  padding: 5px;\n}\n\n.comment_title {\n  color: #025955;\n}\n.comment_input {\n  border: 1px solid #aaa;\n  border-radius: 5px;\n}\n\n.close,\n.dialog_submit {\n  cursor: pointer;\n  position: absolute;\n}\n\n.close {\n  right: 10px;\n  top: 10px;\n}\n\n.dialog_submit {\n  padding: 5px 10px 2px;\n  bottom: 10px;\n  left: 50%;\n  transform: translateX(-50%);\n  border: 2px solid #025955;\n  color: #025955;\n  font-weight: 700;\n  font-size: 1rem;\n  border-radius: 5px;\n  background-color: #fff7e3;\n}\n\n/* footer */\n.footer {\n  padding: 7px 5%;\n  background-color: #022422;\n  color: #efefef;\n  font-size: 0.6rem;\n}\n\n/* icons */\n.fa-times {\n  color: #025955;\n}\n\n/* Drag and Drop */\n.mute-children * {\n  pointer-events: none;\n}\n.lifted {\n  animation: drag_start 250ms;\n  opacity: 0.2;\n}\n\n@keyframes drag_start {\n  0% {\n    opacity: 1;\n  }\n  100% {\n    opacity: 0.2;\n  }\n}\n\n.drop-area {\n  /* transform: translateY(50px); */\n  /* box-shadow:0 0 30px rgba(2, 36, 34,0.5); */\n  border:3px solid rgba(2, 36, 34,0.5);\n}\n"},"lineCount":null}},"error":null,"hash":"22d8f4f844a9aead90d719279bcf4435","cacheData":{"env":{}}}